{
  "global": {
    "show_in_menu_bar": false,
    "show_profile_name_in_menu_bar": false
  },
  "profiles": [
    {
      "name": "Default",
      "complex_modifications": {
        "rules": [
          {
            "description": "Akimbo cmd to F12 (for Leader Key app)",
            "manipulators": [
              {
                "from": {
                  "simultaneous": [
                    { "key_code": "left_command" },
                    { "key_code": "right_command" }
                  ]
                },
                "to": [
                  {
                    "key_code": "f12",
                    "modifiers": ["fn"]
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "vi movement (ctrl + hjkl)",
            "manipulators": [
              {
                "from": {
                  "key_code": "h",
                  "modifiers": {
                    "mandatory": ["control"],
                    "optional": ["caps_lock", "command", "option", "shift", "fn"]
                  }
                },
                "to": [{ "key_code": "left_arrow" }],
                "type": "basic"
              },
              {
                "from": {
                  "key_code": "j",
                  "modifiers": {
                    "mandatory": ["control"],
                    "optional": ["caps_lock", "command", "option", "shift", "fn"]
                  }
                },
                "to": [{ "key_code": "down_arrow" }],
                "type": "basic"
              },
              {
                "from": {
                  "key_code": "k",
                  "modifiers": {
                    "mandatory": ["control"],
                    "optional": ["caps_lock", "command", "option", "shift", "fn"]
                  }
                },
                "to": [{ "key_code": "up_arrow" }],
                "type": "basic"
              },
              {
                "from": {
                  "key_code": "l",
                  "modifiers": {
                    "mandatory": ["control"],
                    "optional": ["caps_lock", "command", "option", "shift", "fn"]
                  }
                },
                "to": [{ "key_code": "right_arrow" }],
                "type": "basic"
              }
            ]
          },
          {
            "description": "mash f+j to escape",
            "manipulators": [
              {
                "from": {
                  "simultaneous": [
                    { "key_code": "f" },
                    { "key_code": "j" }
                  ],
                  "simultaneous_options": {
                    "to_after_key_up": [{ "key_code": "escape" }]
                  }
                },
                "to": [{ "key_code": "vk_none" }],
                "type": "basic"
              }
            ]
          }
        ]
      },
      "simple_modifications": [
        {
          "from": { "key_code": "caps_lock" },
          "to": [{ "key_code": "left_control" }]
        }
      ],
      "devices": [],
      "fn_function_keys": [],
      "virtual_hid_keyboard": {
        "country_code": 0,
        "keyboard_type_v2": "ansi"
      }
    }
  ]
}
